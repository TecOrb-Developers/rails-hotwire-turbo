<main class="container">
  <div class="header">
    <h1>Articles</h1>
    <%= link_to "New Article", new_article_path, class: "btn btn--primary" %>
  </div>
  <table border="1">
    <thead>
      <tr>
        <th>Title</th>
        <th>Auther</th>
        <th>Created at</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <%= render @articles %>
    </tbody>
  </table>
</main>

<tr>
  <%= turbo_frame_tag "article_#{article.id}" do %>
  <td><%= article.id %></td>
  <td><%= link_to article.title, article_path(article) %></td>
  <td><%= article.user.fname %></td>
  <td><%= article.created_at %></td>
  <td>
    <%= button_to "Delete", article_path(article), method: :delete, data: { turbo_confirm: "Are you sure you want delete?" } %>
      <%= link_to "Edit", edit_article_path(article), data: { turbo_frame: "second_frame"} %>
  </td>
  <%end%>
</tr>
